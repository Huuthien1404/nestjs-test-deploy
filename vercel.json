{
    "version": 2,
    "env": {
      "PORT": "@8080"
    },
    "builds": [
      {
        "src": "dist/main.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/main.js",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
      }
    ]
  }